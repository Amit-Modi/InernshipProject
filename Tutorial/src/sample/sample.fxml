<BorderPane prefHeight="330.0" prefWidth="581.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <center>
      <AnchorPane fx:id="scene" BorderPane.alignment="CENTER">
         <children>
            <StackPane fx:id="stackpane" layoutX="368.0" layoutY="49.0">
               <children>
                  <TextArea fx:id="textArea" prefHeight="100.0" prefWidth="100.0" promptText="TextArea">
                     <StackPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </StackPane.margin>
                  </TextArea>
               </children>
            </StackPane>
            <Region fx:id="bottomright" layoutX="478.0" layoutY="159.0" maxHeight="10.0" maxWidth="10.0" minHeight="10.0" minWidth="10.0" prefHeight="10.0" prefWidth="10.0" style="-fx-background-color: blue;" />
            <Region fx:id="bottomleft" layoutX="368.0" layoutY="159.0" maxHeight="10.0" maxWidth="10.0" minHeight="10.0" minWidth="10.0" prefHeight="10.0" prefWidth="10.0" style="-fx-background-color: blue;" />
            <Region fx:id="topright" layoutX="478.0" layoutY="49.0" maxHeight="10.0" maxWidth="10.0" minHeight="10.0" minWidth="10.0" prefHeight="10.0" prefWidth="10.0" style="-fx-background-color: blue;" />
            <Region fx:id="topleft" layoutX="368.0" layoutY="49.0" maxHeight="10.0" maxWidth="10.0" minHeight="10.0" minWidth="10.0" prefHeight="10.0" prefWidth="10.0" style="-fx-background-color: blue;" />
         </children></AnchorPane>
   </center>
   <top>
      <HBox spacing="5.0" style="-fx-background-color: #B8B8B8;" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#addTextField" text="Add TextField" />
            <Button mnemonicParsing="false" onAction="#addBrowser" text="Add browser" />
            <Button mnemonicParsing="false" onAction="#addImage" text="Add Image" />
            <Button mnemonicParsing="false" onAction="#addVideo" text="Add Video" />
            <Button mnemonicParsing="false" onAction="#makeDraggable" text="makeDraggable" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </top>
</BorderPane>
